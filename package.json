{
  "name": "tde-cookie-consent",
  "version": "2.0.0",
  "files": [
    "lib/*"
  ],
  "module": "lib/index.esm.js",
  "main": "lib/index.js",
  "types": "lib/_types/index.d.ts",
  "license": "MIT",
  "config": {
    "node": "18"
  },
  "scripts": {
    "build": "rm -rf ./lib && NODE_ENV=prod node --experimental-modules ./esbuild/build.mjs",
    "watch": "node --experimental-modules ./esbuild/build.mjs --watch",
    "frontend-cs": "pnpm format && pnpm eslint && pnpm stylelint",
    "lint": "pnpm prettier --check . --plugin-search-dir=. && pnpm eslint && pnpm stylelint",
    "eslint": "node esbuild/pre-script.js && eslint src --ext .ts --ignore-path .gitignore; exit 0",
    "eslint-fix": "node esbuild/pre-script.js && eslint src --fix --ext .ts --ignore-path .gitignore; exit 0",
    "format": "node esbuild/pre-script.js && prettier --write . --plugin-search-dir=.",
    "stylelint": "stylelint -f verbose --cache 'src/**/*.{css,scss}'",
    "stylelint-fix": "stylelint -f verbose --cache 'src/**/*.{css,scs}' --fix"
  },
  "author": {
    "name": "TDE",
    "email": "info@tde.nl",
    "url": "https://www.tde.nl/"
  },
  "dependencies": {
    "postscribe": "^2.0.8",
    "svelte": "^3.55.1"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.2",
    "@types/node": "^20.7.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^6.7.3",
    "esbuild": "^0.19.4",
    "esbuild-sass-plugin": "^2.15.0",
    "esbuild-svelte": "^0.8.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "npm-dts": "^1.3.12",
    "prettier": "^2.8.4",
    "prettier-plugin-css-order": "^1.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-preprocess-esbuild": "^3.0.1",
    "tde-stylelint-config": "github:tdeNL/tde-stylelint-config",
    "typescript": "^5.2.2"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not ie <= 10",
      "not op_mini all"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
