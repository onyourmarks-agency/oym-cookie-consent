{
  "name": "tde-cookie-consent",
  "version": "2.0.0",
  "main": "lib/index.js",
  "style": "style.scss",
  "license": "MIT",
  "config": {
    "node": "18"
  },
  "scripts": {
    "build": "node webpack/pre-script.js && webpack --config webpack/webpack.config.js --mode production --stats-children",
    "frontend-cs": "pnpm format && pnpm eslint && pnpm stylelint && pnpm svelte-check",
    "lint": "pnpm prettier --check . --plugin-search-dir=. && pnpm eslint && pnpm stylelint",
    "eslint": "node webpack/pre-script.js && eslint src --ext .ts --ignore-path .gitignore; exit 0",
    "eslint-fix": "node webpack/pre-script.js && eslint src --fix --ext .ts --ignore-path .gitignore; exit 0",
    "format": "node webpack/pre-script.js && prettier --write . --plugin-search-dir=.",
    "stylelint": "stylelint -f verbose --cache 'src/**/*.{css,scss,svelte}'",
    "stylelint-fix": "stylelint -f verbose --cache 'src/**/*.{css,scss,svelte}' --fix",
    "svelte-check": "svelte-check"
  },
  "author": {
    "name": "TDE",
    "email": "info@tde.nl",
    "url": "https://www.tde.nl/"
  },
  "dependencies": {
    "postscribe": "^2.0.8",
    "svelte": "^3.56.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@tsconfig/svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "prettier": "^2.8.4",
    "prettier-plugin-css-order": "^1.3.0",
    "prettier-plugin-svelte": "^2.9.0",
    "stylelint": "^13.13.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "svelte-check": "^3.1.0",
    "svelte-check-plugin": "^1.0.4",
    "svelte-loader": "^3.1.7",
    "svelte-preprocess": "^4.10.7",
    "svelte-preprocess-sass": "^2.0.1",
    "tde-stylelint-config": "github:tdeNL/tde-stylelint-config",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not ie <= 10",
      "not op_mini all"
    ],
    "modern": [
      "last 1 chrome version",
      "last 1 firefox version"
    ]
  }
}
