{
  "name": "tde-cookie-consent",
  "version": "2.0.0",
  "files": [
    "lib/*"
  ],
  "module": "lib/index.esm.js",
  "main": "lib/index.js",
  "types": "lib/_types/index.d.ts",
  "license": "MIT",
  "config": {
    "node": "18"
  },
  "scripts": {
    "watch": "node webpack/pre-script.js && webpack --watch --config webpack/webpack.config.js --stats-children",
    "pre": "node webpack/pre-script.js",
    "build": "node webpack/pre-script.js && NODE_ENV=prod webpack --mode production --config webpack/webpack.config.js --stats-children",
    "frontend-cs": "pnpm format && pnpm eslint && pnpm stylelint && pnpm svelte-check",
    "lint": "pnpm prettier --check . --plugin-search-dir=. && pnpm eslint && pnpm styzlelint",
    "eslint": "node webpack/pre-script.js && eslint assets --cache --ext .js,.svelte --ignore-path .gitignore",
    "eslint-fix": "node webpack/pre-script.js && eslint assets --cache --fix --ext .js,.svelte --ignore-path .gitignore",
    "format": "node webpack/pre-script.js && prettier --write . --plugin-search-dir=.",
    "format-az": "node webpack/pre-script.js && ORDER=alphabetical prettier --write . --plugin-search-dir=",
    "stylelint": "stylelint -f verbose --cache 'assets/**/*.{css,scss,svelte}'",
    "stylelint-fix": "stylelint -f verbose --cache 'assets/**/*.{css,scss,svelte}' --fix",
    "svelte-check": "svelte-check"
  },
  "author": {
    "name": "TDE",
    "email": "info@tde.nl",
    "url": "https://www.tde.nl/"
  },
  "dependencies": {
    "postscribe": "^2.0.8",
    "svelte": "^3.55.1"
  },
  "devDependencies": {
